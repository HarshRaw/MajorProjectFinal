@model IEnumerable<MajorProject.Models.Car>

@{
    ViewData["Title"] = "Index";
}
<div style="padding-top:6rem;padding-left:2rem;padding-right:2rem">


<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    Customer Id
                </th>
                <th>
                    Customer Name
                </th>
            <th>
                @Html.DisplayNameFor(model => model.CarModels)
            </th>
            <th>
                Company
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CarNumber)
            </th>
               
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Customers.CustomerId)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Customers.CustomerName)
                    </td>
                    
            <td>
                @Html.DisplayFor(modelItem => item.CarModels.CarModels)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CarModels.CarCompanies.CarCompanies)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CarNumber)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CarId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CarId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CarId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</div>
